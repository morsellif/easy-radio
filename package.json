{
	"name": "easy-radio",
	"packageManager": "pnpm@8.11.0",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/morsellif/easy-radio.git"
	},
	"version": "0.0.0",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
