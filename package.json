{
	"name": "easy-radio",
	"packageManager": "pnpm@8.3.1",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/morsellif/easy-radio.git"
	},
	"version": "0.0.0",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "nuxt dev",
		"build": "nuxt build",
		"generate": "nuxt generate",
		"typecheck": "nuxt typecheck",
		"postinstall": "nuxt prepare",
